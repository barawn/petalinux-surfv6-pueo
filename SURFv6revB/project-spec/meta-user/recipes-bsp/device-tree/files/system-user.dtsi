/include/ "system-conf.dtsi"
/ {
   reserved-memory {
     #address-cells = <2>;
     #size-cells = <2>;
     ranges;

     reserved: buffer@0x70000000 {
        no-map;
	reg = <0x0 0x70000000 0x0 0x100000 >;
     };
   };
   reserved-driver@0 {
     compatible = "xlnx,reserved-memory";
     memory-region = <&reserved>;
   };
};

#include "include/dt-bindings/phy/phy.h"
#include "include/dt-bindings/gpio/gpio.h"


&sdhci1 {
	disable-wp;
	no-1-8-v;
};

&sdhci0 {
	disable-wp;
	no-1-8-v;
};

&spi0 {
      spidev@0 {
      	       compatible = "ti,lmk0461x";
	       reg = <0>;
	       num_bytes = <3>;
	       spi-max-frequency = <500000>;
      };
};

&spi1 {
      spidev@0 {
      	       compatible = "osu,surf6revB";
	       reg = <0>;
	       num_bytes = <7>;
	       spi-max-frequency = <10000000>;
      };
};

&gem3 {
      status = "okay";
      ethernet_phy0: ethernet-phy@0 {
      		     compatible = "marvell,88e1510";
		     device_type = "ethernet-phy";
		     reset-gpios = <&gpio 36 GPIO_ACTIVE_LOW>;
		     reset-assert-us = <15000>;
		     reset-deassert-us = <15000>;
		     reg = <1>;
      };
};

&qspi {
      u-boot,dm-pre-reloc;
      #address-cells = <1>;
      #size-cells = <0>;
      status = "okay";
      is-dual = <1>;
      is-stacked = <0>;
      num-cs = <2>;
      has-io-mode = <1>;
      qspi-mode = <1>;      
      flash0: flash@0 {
              compatible = "jedec,spi-nor";
              reg = <0x0>;
              #address-cells = <1>;
              #size-cells = <1>;
              spi-tx-bus-width = <1>;
              spi-rx-bus-width = <4>;
              spi-max-frequency = <50000000>;
              parallel-memories = /bits/ 64 <0x4000000 0x4000000>; /* 128 MB */
	      partition@0x00000000 {
	        label = "boot";
		reg = <0x0 0x01e0000>;
	      };
	      partition@0x01e0000 {
	        label = "bootscr";
		reg = <0x01e0000 0x20000>;
	      };
	      partition@0x0200000 {
	        label = "qspifs";
		reg = <0x0200000 0x7e00000>;
	      };
	      
      };
};

&i2c0 {
      usbc: usbc@1D {
      	    compatible = "pericom,pi5usb30216d";
	    reg = <0x1D>;
      };
};      

&i2c1 {
      eeprom: eeprom@50 {
      	      compatible = "microchip,24aa025", "atmel,24c02";
	      reg = <0x50>;
      };
};

&dcc {
     status = "okay";
};

